proc</l></list><l>if (pixel &gt;= 0 &amp;&amp; pixel &lt;= pixelCount) {&#xD;  pixel = Math.floor(pixel);&#xD;  var msg = "setpixel " + String(pixel) + " " + String(red) + " " + String(green) + " " + String(blue);&#xD;  window.socket.send(msg);&#xD;}&#xD;</l></block><list><block var="pixel"/><custom-block s="kleurLimiet %n"><block var="rood"/></custom-block><custom-block s="kleurLimiet %n"><block var="groen"/></custom-block><custom-block s="kleurLimiet %n"><block var="blauw"/></custom-block></list></block></block></script></block-definition><block-definition s="Verschuif pixels %&apos;Richting&apos;" type="command" category="OWLS"><header></header><code></code><translations></translations><inputs><input type="%txt">Links<options>Links&#xD;Rechts</options></input></inputs><script><block s="doReport"><block s="evaluate"><block s="reportJSFunction"><list><l>direction</l><l>proc</l></list><l>if (direction == "Left" || direction == "Links") {&#xD;  window.socket.send("shift left");&#xD;} else {&#xD;  window.socket.send("shift right");&#xD;}&#xD;</l></block><list><block var="Richting"/></list></block></block></script></block-definition><block-definition s="Maak pixels %&apos;dim&apos; donker" type="command" category="OWLS"><header></header><code></code><translations></translations><inputs><input type="%n">1</input></inputs><script><block s="doReport"><block s="evaluate"><block s="reportJSFunction"><list><l>dimAmount</l><l>proc</l></list><l>var msg = "dim " + String(dimAmount);&#xD;window.socket.send(msg);&#xD;</l></block><list><custom-block s="kleurLimiet %n"><block var="dim"/></custom-block></list></block></block></script></block-definition><block-definition s="kleurLimiet %&apos;kleurwaarde&apos;" type="reporter" category="OWLS"><header></header><code></code><translations></translations><inputs><input type="%n"></input></inputs><script><block s="doReport"><block s="evaluate"><block s="reportJSFunction"><list><l>color</l></list><l>color = Math.floor(color);&#xD;color = parseInt(color);&#xD;color = Math.min(255,color);&#xD;color = Math.max(0,color);&#xD;return color;&#xD;&#xD;</l></block><list><block var="kleurwaarde"/></list></block></block></script></block-definition><block-definition s="Direct zien %&apos;status&apos;" type="command" category="OWLS"><header></header><code></code><translations></translations><inputs><input type="%txt">Aan<options>Aan&#xD;Uit</options></input></inputs><script><block s="doRun"><block s="reportJSFunction"><list><l>autoShowValue</l><l>proc</l></list><l>if (autoShowValue == "On" || autoShowValue == "Aan") {&#xD;  window.socket.send("autoshow on");&#xD;} else {&#xD;  window.socket.send("autoshow off");&#xD;}&#xD;</l></block><list><block var="status"/></list></block></script></block-definition><block-definition s="Toon pixels" type="command" category="OWLS"><header></header><code></code><translations></translations><inputs></inputs><script><block s="doRun"><block s="reportJSFunction"><list><l>proc</l></list><l>window.socket.send("show");</l></block><list></list></block></script></block-definition><block-definition s="Stel aantal pixels in op %&apos;aantalPixels&apos;" type="command" category="OWLS"><header></header><code></code><translations></translations><inputs><input type="%n">8</input></inputs><script><block s="doRun"><block s="reportJSFunction"><list><l>nrPixels</l><l>proc</l></list><l>nrPixels = Math.floor(nrPixels);&#xD;nrPixels = Math.min(realPixelCount, nrPixels);&#xD;nrPixels = Math.max(1,nrPixels);&#xD;var msg = "setVirtualPixels " + String(nrPixels);&#xD;window.socket.send(msg);&#xD;&#xD;// Set the pixel count.&#xD;pixelCount = parseInt(nrPixels);&#xD;</l></block><list><block var="aantalPixels"/></list></block></script></block-definition><block-definition s="kleurgradient van %&apos;vanRood&apos; , %&apos;vanGroen&apos; , %&apos;vanBlauw&apos; naar %&apos;naarRood&apos; , %&apos;naarGroen&apos; , %&apos;naarBlauw&apos;" type="command" category="OWLS"><header></header><code></code><translatio